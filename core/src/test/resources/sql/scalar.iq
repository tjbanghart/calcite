| DEPTNO | X     |
+--------+-------+
|     10 | 23555 |
|     20 | 38501 |
|     30 | 46116 |
|     40 |       |
+--------+-------+
(4 rows)

!ok

select deptno, (select count(*) from "scott".emp where 1 = 0) as x from "scott".dept;
