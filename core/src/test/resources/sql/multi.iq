!use scott
!set outputformat csv

# Skip all HepPlanner preprocessing (including calc conversion)
# and go directly to VolcanoPlanner for optimization
!set volcanoonly true
# Add CombineSharedComponentsRule - spool conversion should happen automatically
# (BINDABLE_TABLE_SCAN_RULE and TO_INTERPRETER are already registered by default)
!set planner-rules "+COMBINE_SHARED_COMPONENTS"

# Test basic MULTI with two simple queries - execution
MULTI(
    (SELECT EMPNO, SAL FROM (SELECT * FROM EMP WHERE SAL > 1000)),
    (SELECT ENAME, DEPTNO FROM (SELECT * FROM EMP WHERE SAL > 1000))
);
!plan
!ok

MULTI(
    (SELECT EMPNO, SAL FROM (SELECT * FROM EMP WHERE SAL > 100)),
    (SELECT ENAME, DEPTNO FROM (SELECT * FROM EMP WHERE SAL > 100)),
    (SELECT JOB, MGR FROM (SELECT * FROM EMP WHERE SAL < 10)),
    (SELECT HIREDATE, COMM FROM (SELECT * FROM EMP WHERE SAL < 10))
);
#!plan
#!ok


# End multi.iq
