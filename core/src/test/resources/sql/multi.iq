!use scott
!set outputformat mysql

# Skip all HepPlanner preprocessing (including calc conversion)
# and go directly to VolcanoPlanner for optimization
!set volcanoonly true

!set planner-rules "+COMBINE_SHARED_COMPONENTS"
# Test basic MULTI with two simple queries - execution
MULTI(
    (SELECT EMPNO, SAL FROM (SELECT * FROM EMP WHERE SAL > 1000)),
    (SELECT ENAME, DEPTNO FROM (SELECT * FROM EMP WHERE SAL > 1000))
);
EnumerableCombine
  EnumerableProject(EMPNO=[$0], SAL=[$5])
    EnumerableFilter(condition=[>(CAST($5):DECIMAL(12, 2), 1000.00)])
      EnumerableTableSpool(readType=[LAZY], writeType=[LAZY], table=[[TEMP, spool_0]])
        EnumerableTableScan(table=[[scott, EMP]])
  EnumerableProject(ENAME=[$1], DEPTNO=[$7])
    EnumerableFilter(condition=[>(CAST($5):DECIMAL(12, 2), 1000.00)])
      EnumerableInterpreter
        BindableTableScan(table=[[TEMP, spool_0]])
!plan
+--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------+--------------------------------------------------------------------------------------------------------------------------------------------------------------+
| EXPR$0                                                                                                                                                                                                       | EXPR$1                                                                                                                                                       |
+--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------+--------------------------------------------------------------------------------------------------------------------------------------------------------------+
| [{7499, 1600.00}, {7521, 1250.00}, {7566, 2975.00}, {7654, 1250.00}, {7698, 2850.00}, {7782, 2450.00}, {7788, 3000.00}, {7839, 5000.00}, {7844, 1500.00}, {7876, 1100.00}, {7902, 3000.00}, {7934, 1300.00}] | [{ALLEN, 30}, {WARD, 30}, {JONES, 20}, {MARTIN, 30}, {BLAKE, 30}, {CLARK, 10}, {SCOTT, 20}, {KING, 10}, {TURNER, 30}, {ADAMS, 20}, {FORD, 20}, {MILLER, 10}] |
+--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------+--------------------------------------------------------------------------------------------------------------------------------------------------------------+
(1 row)

!ok
MULTI(
    (SELECT EMPNO, SAL FROM (SELECT * FROM EMP WHERE SAL > 1000)),
    (SELECT ENAME, DEPTNO FROM (SELECT * FROM EMP WHERE SAL > 1000)),
    (SELECT JOB, MGR FROM (SELECT * FROM EMP WHERE SAL < 1000)),
    (SELECT HIREDATE, COMM FROM (SELECT * FROM EMP WHERE SAL < 1000))
);
EnumerableCombine
  EnumerableProject(EMPNO=[$0], SAL=[$5])
    EnumerableFilter(condition=[>(CAST($5):DECIMAL(12, 2), 1000.00)])
      EnumerableTableSpool(readType=[LAZY], writeType=[LAZY], table=[[TEMP, spool_0]])
        EnumerableTableScan(table=[[scott, EMP]])
  EnumerableProject(ENAME=[$1], DEPTNO=[$7])
    EnumerableFilter(condition=[>(CAST($5):DECIMAL(12, 2), 1000.00)])
      EnumerableInterpreter
        BindableTableScan(table=[[TEMP, spool_0]])
  EnumerableProject(JOB=[$2], MGR=[$3])
    EnumerableFilter(condition=[<(CAST($5):DECIMAL(12, 2), 1000.00)])
      EnumerableInterpreter
        BindableTableScan(table=[[TEMP, spool_0]])
  EnumerableProject(HIREDATE=[$4], COMM=[$6])
    EnumerableFilter(condition=[<(CAST($5):DECIMAL(12, 2), 1000.00)])
      EnumerableInterpreter
        BindableTableScan(table=[[TEMP, spool_0]])
!plan
+--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------+--------------------------------------------------------------------------------------------------------------------------------------------------------------+--------------------------------+------------------------------------------+
| EXPR$0                                                                                                                                                                                                       | EXPR$1                                                                                                                                                       | EXPR$2                         | EXPR$3                                   |
+--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------+--------------------------------------------------------------------------------------------------------------------------------------------------------------+--------------------------------+------------------------------------------+
| [{7499, 1600.00}, {7521, 1250.00}, {7566, 2975.00}, {7654, 1250.00}, {7698, 2850.00}, {7782, 2450.00}, {7788, 3000.00}, {7839, 5000.00}, {7844, 1500.00}, {7876, 1100.00}, {7902, 3000.00}, {7934, 1300.00}] | [{ALLEN, 30}, {WARD, 30}, {JONES, 20}, {MARTIN, 30}, {BLAKE, 30}, {CLARK, 10}, {SCOTT, 20}, {KING, 10}, {TURNER, 30}, {ADAMS, 20}, {FORD, 20}, {MILLER, 10}] | [{CLERK, 7902}, {CLERK, 7698}] | [{1980-12-17, null}, {1981-12-03, null}] |
+--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------+--------------------------------------------------------------------------------------------------------------------------------------------------------------+--------------------------------+------------------------------------------+
(1 row)

!ok


